draw_a_name <- function(the_name,the_sex){
asserthat(is.character(the_name))
asserthat(the_sex == c("M","F"))
base <- prenoms::prenoms %>%
filter(name == the_name & sex == the_sex) %>%
group_by(year) %>%
summarize(total=sum(n))
ggplot(data = base, aes(x = year, y = total)) +
geom_line()
}
